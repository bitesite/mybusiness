<div class="container">
	<p id="notice"><%= notice %></p>

	<h1><%= @contest.name %></h1>
	
	<% if admin? %>
	<h2>Contestants</h2>
	<ul>
		<% @contest.contestants.each do |c| %>
			<li><%= "#{c.first_name} #{c.last_name} / #{c.email} / #{c.notes}" %></li>
		<% end %>
	</ul>


	<%= link_to 'Edit', edit_contest_path(@contest) %> |
	<%= link_to 'Back', contests_path %>	
	<% end %>
</div>

